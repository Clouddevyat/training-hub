{
  "meta": {
    "id": "combatAlpinist",
    "name": "Combat Alpinist",
    "description": "Integrated mountaineering protocol - 40 week periodized program. Build aerobic engine, convert strength to muscular endurance, peak for expeditions.",
    "version": "2.0",
    "type": "standalone",
    "icon": "mountain",
    "author": "Training Hub"
  },
  "required_profile_fields": [
    "bodyweight",
    "1rm.trap_bar_deadlift",
    "1rm.weighted_pullup"
  ],
  "equipment_required": [
    "trap_bar",
    "pull_up_bar",
    "dip_station",
    "box",
    "dumbbells",
    "weight_vest"
  ],
  "globalRules": {
    "deload_every_4th_week": true,
    "one_specialty_at_a_time": true,
    "always_return_to_base": true
  },
  "quarterlyTesting": {
    "frequency": "every_12_weeks",
    "tests": [
      { "name": "AeT Drift Test", "protocol": "60 min @ AeT HR, measure drift", "track": ["hr_drift_percent"] },
      { "name": "5-Mile Run", "protocol": "All-out effort on flat terrain", "track": ["time"] },
      { "name": "Strength Check", "protocol": "Trap bar deadlift to heavy single", "track": ["weight"] },
      { "name": "Loaded Carry", "protocol": "25% BW pack, max distance in 60 min", "track": ["distance", "vert"] }
    ]
  },
  "phases": [
    {
      "id": "foundation",
      "name": "Foundation",
      "weeks": [1, 16],
      "description": "Build aerobic engine, maintain strength. 80% Zone 1-2, 20% Zone 3-4. Volume: Start 8-10 hrs/week, build to 12-14 hrs/week by week 16.",
      "exitCriteria": [
        "AeT HR drift <5% over 60 min steady state",
        "5-mile run under 36:00",
        "Strength floor maintained (no lift drops >5%)"
      ],
      "benchmarks": [
        { "name": "AeT HR Drift", "target": "<5%" },
        { "name": "5-Mile Run", "target": "<36:00" },
        { "name": "Strength", "target": "Maintained" }
      ],
      "weeklyTemplate": [
        {
          "day": 1,
          "session": "Strength A",
          "type": "strength",
          "duration": 60,
          "prescription": {
            "warmup": "10 min easy cardio + dynamic stretching",
            "exercises": [
              { "name": "Trap Bar Deadlift", "sets": 4, "reps": "3-5", "rest": "3 min", "prKey": "trapBarDeadlift", "percentage": 85 },
              { "name": "Box Step-Up", "sets": 4, "reps": "3-5/leg", "rest": "3 min", "prKey": "boxStepUp" },
              { "name": "Weighted Pull-Up", "sets": 4, "reps": "3-5", "rest": "3 min", "prKey": "weightedPullUp", "percentage": 85 },
              { "name": "Dip", "sets": 4, "reps": "3-5", "rest": "3 min", "prKey": "weightedDip", "percentage": 85 }
            ],
            "cooldown": "5 min walk",
            "intensity": "80-85% 1RM, never to failure",
            "notes": ["Quality over volume", "Full ATP-CP recovery between sets"]
          }
        },
        {
          "day": 2,
          "session": "Zone 2 Run",
          "type": "cardio",
          "duration": 70,
          "prescription": {
            "description": "Conversational pace on flat terrain",
            "hrZone": "zone2",
            "notes": ["Nasal breathing preferred", "If HR drifts up, slow down"]
          }
        },
        {
          "day": 3,
          "session": "Zone 2 + Mobility",
          "type": "cardio",
          "duration": 55,
          "prescription": {
            "description": "30-40 min easy effort + 20 min yoga/mobility",
            "hrZone": "zone2",
            "notes": ["Pigeon pose for hips", "Cat-Cow for thoracic spine", "Child's pose for lower back"]
          }
        },
        {
          "day": 4,
          "session": "Threshold Intervals",
          "type": "cardio",
          "duration": 45,
          "prescription": {
            "warmup": "10 min easy",
            "mainSet": "4 x 8 min at AeT",
            "recovery": "2 min jog",
            "hrZone": "zone3",
            "cooldown": "5-10 min easy"
          }
        },
        {
          "day": 5,
          "session": "Strength B",
          "type": "strength",
          "duration": 60,
          "prescription": {
            "warmup": "10 min easy cardio",
            "exercises": [
              { "name": "Trap Bar Deadlift", "sets": 4, "reps": "3-5", "rest": "3 min", "prKey": "trapBarDeadlift", "percentage": 85 },
              { "name": "Box Step-Up", "sets": 4, "reps": "3-5/leg", "rest": "3 min", "prKey": "boxStepUp" },
              { "name": "Weighted Pull-Up", "sets": 4, "reps": "3-5", "rest": "3 min", "prKey": "weightedPullUp", "percentage": 85 },
              { "name": "Dip", "sets": 4, "reps": "3-5", "rest": "3 min", "prKey": "weightedDip", "percentage": 85 }
            ],
            "accessory": [
              { "name": "Face Pulls", "sets": 2, "reps": "15" },
              { "name": "Pallof Press", "sets": 2, "reps": "15/side" }
            ],
            "cooldown": "5 min walk"
          }
        },
        {
          "day": 6,
          "session": "Long Aerobic",
          "type": "long_effort",
          "duration": 150,
          "prescription": {
            "description": "Long Zone 1-2 effort: hike or run, unloaded, conversational pace",
            "hrZone": "zone2",
            "options": ["Long hike", "Long run", "Combo"],
            "notes": ["2-3 hours", "Nutrition: 30-60g carbs/hr after first hour"]
          }
        },
        {
          "day": 7,
          "session": "Active Recovery",
          "type": "recovery",
          "duration": 30,
          "prescription": {
            "description": "Walk, mobility, nothing strenuous",
            "options": ["Easy walk", "Light yoga", "Swimming", "Foam rolling"]
          }
        }
      ]
    },
    {
      "id": "conversion",
      "name": "Conversion",
      "weeks": [17, 28],
      "description": "Convert raw strength to muscular endurance. Introduce loaded carries. The crux of the physical preparation.",
      "exitCriteria": [
        "600 box steps with 20% BW, continuous",
        "90 min uphill at 20% BW, HR stays Zone 2",
        "5-mile under 34:00",
        "AeT within 12% of AnT"
      ],
      "benchmarks": [
        { "name": "Gym ME", "target": "600 steps @ 20% BW" },
        { "name": "5-Mile", "target": "<34:00" },
        { "name": "AeT/AnT Gap", "target": "<12%" }
      ],
      "weeklyTemplate": [
        {
          "day": 1,
          "session": "Strength (Reduced)",
          "type": "strength",
          "duration": 45,
          "prescription": {
            "warmup": "10 min easy",
            "exercises": [
              { "name": "Trap Bar Deadlift", "sets": 3, "reps": "3-5", "rest": "3 min", "prKey": "trapBarDeadlift", "percentage": 85 },
              { "name": "Box Step-Up", "sets": 3, "reps": "3-5/leg", "rest": "3 min", "prKey": "boxStepUp" },
              { "name": "Weighted Pull-Up", "sets": 3, "reps": "3-5", "rest": "3 min", "prKey": "weightedPullUp", "percentage": 85 },
              { "name": "Dip", "sets": 3, "reps": "3-5", "rest": "3 min", "prKey": "weightedDip", "percentage": 85 }
            ],
            "intensity": "Maintenance only - 3 sets each"
          }
        },
        {
          "day": 2,
          "session": "Zone 2 Run",
          "type": "cardio",
          "duration": 60,
          "prescription": {
            "description": "Unloaded, recovery pace",
            "hrZone": "zone2"
          }
        },
        {
          "day": 3,
          "session": "Gym ME Session",
          "type": "muscular_endurance",
          "duration": 70,
          "prescription": {
            "warmup": "10 min easy",
            "description": "High volume, minimal rest, continuous movement. Tempo: 1 step every 2 seconds. No rest between exercises.",
            "loadType": "bodyweight_percentage",
            "load": 0,
            "progression": {
              "Weeks 17-20": { "load": 0, "steps": "400 steps (200/leg)", "note": "Bodyweight only" },
              "Weeks 21-24": { "load": 10, "steps": "500 steps" },
              "Weeks 25-28": { "load": 20, "steps": "600 steps" }
            },
            "exercises": [
              { "name": "Box Step-Up", "sets": 1, "reps": "continuous circuit" },
              { "name": "Walking Lunge", "sets": 1, "reps": "continuous circuit" },
              { "name": "Split Squat", "sets": 1, "reps": "continuous circuit" },
              { "name": "Heel Touch", "sets": 1, "reps": "continuous circuit" }
            ],
            "notes": ["Box at 75% knee height", "Eccentric focus on heel touches", "1 step every 2 seconds tempo"]
          }
        },
        {
          "day": 4,
          "session": "Zone 2 + Mobility",
          "type": "cardio",
          "duration": 45,
          "prescription": {
            "description": "30 min easy spin or walk + 15 min yoga",
            "hrZone": "zone2"
          }
        },
        {
          "day": 5,
          "session": "Threshold Intervals",
          "type": "cardio",
          "duration": 45,
          "prescription": {
            "warmup": "10 min building",
            "mainSet": "5 x 6 min at tempo",
            "recovery": "2 min jog",
            "hrZone": "zone3",
            "cooldown": "5-10 min easy"
          }
        },
        {
          "day": 6,
          "session": "Outdoor ME (Water Jug)",
          "type": "muscular_endurance",
          "duration": 90,
          "prescription": {
            "description": "THE WATER JUG PROTOCOL - Loaded uphill, unloaded descent",
            "loadType": "bodyweight_percentage",
            "load": 15,
            "progression": {
              "Weeks 17-20": { "load": 15, "duration": "60-90 min uphill" },
              "Weeks 21-24": { "load": 20, "duration": "60-90 min uphill" },
              "Weeks 25-28": { "load": 25, "duration": "60-90 min uphill" }
            },
            "steps": [
              "Fill pack to target weight",
              "Find steep terrain 30%+ grade",
              "Hike up 60-90 min",
              "DUMP WATER at top",
              "Descend empty - spare your joints, buy training frequency"
            ],
            "hrZone": "zone2",
            "notes": ["Stairmaster at 15%+ incline simulates this"]
          }
        },
        {
          "day": 7,
          "session": "Active Recovery",
          "type": "recovery",
          "duration": 30,
          "prescription": {
            "description": "Full rest or light mobility",
            "hrZone": "zone1"
          }
        }
      ]
    },
    {
      "id": "specificity",
      "name": "Specificity",
      "weeks": [29, 40],
      "description": "Expedition simulation. Back-to-back efforts, peak loads, operational conditions. Sawtooth training pattern.",
      "exitCriteria": [
        "1,000 ft/hr vertical at 25% BW in Zone 2",
        "32:00 5-mile",
        "Back-to-back days without performance degradation",
        "AeT within 10% of AnT (diesel engine achieved)"
      ],
      "benchmarks": [
        { "name": "Vertical Rate", "target": "1000 ft/hr @ 25% BW" },
        { "name": "5-Mile", "target": "<32:00" },
        { "name": "AeT/AnT Gap", "target": "<10%" }
      ],
      "weeklyTemplate": [
        {
          "day": 1,
          "session": "Strength (Minimal)",
          "type": "strength",
          "duration": 40,
          "prescription": {
            "exercises": [
              { "name": "Trap Bar Deadlift", "sets": 3, "reps": "3-5", "prKey": "trapBarDeadlift", "percentage": 82 },
              { "name": "Weighted Pull-Up", "sets": 3, "reps": "3-5", "prKey": "weightedPullUp", "percentage": 82 }
            ],
            "intensity": "2-3 lifts, maintenance intensity"
          }
        },
        {
          "day": 2,
          "session": "Zone 2 Run",
          "type": "cardio",
          "duration": 55,
          "prescription": {
            "description": "Active recovery effort",
            "hrZone": "zone2"
          }
        },
        {
          "day": 3,
          "session": "Peak ME Session",
          "type": "muscular_endurance",
          "duration": 75,
          "prescription": {
            "description": "Circuit at peak load. Tempo: 1 step every 2 seconds.",
            "loadType": "bodyweight_percentage",
            "load": 25,
            "exercises": [
              { "name": "Box Step-Up", "sets": 1, "reps": "continuous circuit" },
              { "name": "Walking Lunge", "sets": 1, "reps": "continuous circuit" },
              { "name": "Split Squat", "sets": 1, "reps": "continuous circuit" },
              { "name": "Heel Touch", "sets": 1, "reps": "continuous circuit" }
            ],
            "notes": ["Target: 800-1000 steps at 25% BW", "1 step every 2 seconds tempo"]
          }
        },
        {
          "day": 4,
          "session": "Off or Mobility",
          "type": "recovery",
          "duration": 30,
          "prescription": {
            "description": "TRUE RECOVERY - needed for sawtooth weekend",
            "options": ["Complete rest", "Light mobility only"]
          }
        },
        {
          "day": 5,
          "session": "Tempo Run",
          "type": "cardio",
          "duration": 45,
          "prescription": {
            "warmup": "10 min building",
            "mainSet": "3 x 1 mile at goal pace",
            "recovery": "2-3 min jog",
            "cooldown": "10 min easy",
            "notes": ["Goal pace work for 32-min 5-mile prep"]
          }
        },
        {
          "day": 6,
          "session": "Sawtooth Day 1 - Long Loaded",
          "type": "long_effort",
          "duration": 300,
          "prescription": {
            "description": "LONG LOADED EFFORT - Sawtooth day 1. Target: 4-6 hours loaded.",
            "loadType": "bodyweight_percentage",
            "load": 27,
            "hrZone": "zone2",
            "notes": ["25-30% BW (56-67 lbs)", "Benchmark: 1000 ft/hr in Zone 2", "Nutrition: 60-90g carbs/hr", "Train fatigue resistance"]
          }
        },
        {
          "day": 7,
          "session": "Sawtooth Day 2 - Moderate Aerobic",
          "type": "long_effort",
          "duration": 150,
          "prescription": {
            "description": "BACK-TO-BACK - Train recovery under accumulated stress. Target: 2-3 hours moderate.",
            "loadType": "bodyweight_percentage",
            "load": 15,
            "hrZone": "zone2",
            "notes": ["15% BW or unloaded option", "Focus on recovery under accumulated fatigue"]
          }
        }
      ]
    },
    {
      "id": "transition",
      "name": "Transition",
      "weeks": [41, 52],
      "description": "Active recovery. Volume drops to 6-8 hrs/week. Intensity Zone 1-2 only. No threshold work. Physiological and psychological reset.",
      "exitCriteria": [
        "Feel refreshed and ready for next macrocycle",
        "Nagging injuries addressed",
        "Sleep and nutrition optimized"
      ],
      "benchmarks": [
        { "name": "Recovery", "target": "Complete" },
        { "name": "AeT/AnT Test", "target": "Confirm diesel engine" }
      ],
      "weeklyTemplate": [
        {
          "day": 1,
          "session": "Light Movement",
          "type": "recovery",
          "duration": 45,
          "prescription": {
            "description": "General movement, mobility focus",
            "hrZone": "zone1"
          }
        },
        {
          "day": 2,
          "session": "Easy Cardio",
          "type": "cardio",
          "duration": 45,
          "prescription": {
            "description": "Zone 1-2 only",
            "hrZone": "zone2"
          }
        },
        {
          "day": 3,
          "session": "Skill Work or Course",
          "type": "skill",
          "duration": 60,
          "prescription": {
            "description": "Technical skill work - climbing, rope systems, navigation",
            "notes": ["Pursue technical skill courses", "Build sleep surplus", "Optimize nutrition"]
          }
        },
        {
          "day": 4,
          "session": "Rest or Mobility",
          "type": "recovery",
          "duration": 30,
          "prescription": {
            "description": "Address nagging injuries with dedicated rehab"
          }
        },
        {
          "day": 5,
          "session": "Easy Cardio",
          "type": "cardio",
          "duration": 45,
          "prescription": {
            "description": "Zone 1-2 only",
            "hrZone": "zone2"
          }
        },
        {
          "day": 6,
          "session": "Optional Activity",
          "type": "recovery",
          "duration": 60,
          "prescription": {
            "description": "Whatever sounds fun - hike, swim, climb",
            "hrZone": "zone1"
          }
        },
        {
          "day": 7,
          "session": "Rest",
          "type": "recovery",
          "duration": 0,
          "prescription": {
            "description": "Complete rest"
          }
        }
      ]
    }
  ],
